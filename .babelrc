{
  "presets": ["next/babel"],
  "plugins": [
    ["@babel/plugin-proposal-decorators", { "legacy": true }],
    [
      "@babel/plugin-proposal-class-properties",
      {
        "loose": false
      }
    ],
    ["import", { "libraryName": "antd", "style": true }],
    [
      "babel-plugin-styled-components",
      {
        "ssr": false
      }
    ]
  ]
}
